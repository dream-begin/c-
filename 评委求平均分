#include<stdio.h>
int main()
{
	int score[10];
	int max, min, total = 0;
	printf("请输入评分\n");
	for (int i = 0; i < 10; i++)
	{
		scanf_s("%d", &score[i]);
	}
	for (int i = 0; i < 10; i++)
	{
		max = score[0];
		if (score[i] > max)
			max = score[i];
	}
	for (int i = 0; i < 10; i++)
	{
		min = score[0];
		if (score[i] < min)
			min = score[i];
	}
	for (int i = 0; i < 10; i++)
	{
		if (score[i] != max && score[i] != min)
			total += score[i];
	}
	float avg;
	avg = total / 8.0;
	printf("平均分是%.2f", avg);



	return 0;
}
